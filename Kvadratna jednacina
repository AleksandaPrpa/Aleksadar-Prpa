#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include <string.h>
int main()
{
	int a,b,c;//clanovi uz nepoznatu
	int x,y;//x-duzina y-visina
	float f;//resenje
	char o[2];
	x=20;
	y=2;
	int m[y][x];
	p:
	printf("Unesi vrednost za clan koji je uz n%c\n",253);
	scanf("%d",&a);
	printf("Unesi vrednost za clan koji je uz n\n");
	scanf("%d",&b);
	printf("Unesi vrednost za slobodan clan\n");
	scanf("%d",&c);
	printf("Da li je ovako\n");
	m[0][0]=a;
	m[0][1]=110;//n
	m[0][2]=253;//2
	m[0][3]=43;//+
	m[0][4]=b;
	m[0][5]=110;//n
	m[0][6]=43;//+
	m[0][7]=c;
	printf("%d%c%c%c%d%c%c%d\n",m[0][0],m[0][1],m[0][2],m[0][3],m[0][4],m[0][5],m[0][6],m[0][7]);
	printf("Unesi:\nDa-Nastavlja se racunanje\nNe-Ponovo se unosi vrednosti\n");
	g:
	gets(o);
	if(strcmpi(o,"da")==0)
	{}
	else
	{
		if(strcmpi(o,"ne")==0)
		{
			goto p;
		}
		else
		{
			printf("Pogresan unos\n");
			goto g;
		}
	}	
	m[1][1]=;
	m[1][2]=2;
	printf("x  ");
	return 0;
}
