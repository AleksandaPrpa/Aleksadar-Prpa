                                    drugi_igrac_sakriveno[red1-1][kolona1-1]=79;
                                }
                            }
                            return 2;
                        }
                        else
                        {
                            return 1;
                        }
                    }
                    else
                    {
                        return 1;
                    }
                }
                else
                {
                    return 1;
                }
    }
    else
    {
        drugi_igrac[red1][kolona1]=79;
        drugi_igrac_sakriveno[red1][kolona1]=79;
        return 0;
    }
}
int unos_brodova_bot(int n,int m,int bot[n][m])
{
    int i,j;
    for(i=0;i<12;i++)
    {
        for(j=0;j<12;j++)
        {
            bot[i][j]=88;
        }
    }
    bot_brod_4(n,m,bot);
    bot_brod_3(n,m,bot);
    bot_brod_2(n,m,bot);
    bot_brod_1(n,m,bot);
}
void bot_brod_1(int n,int m,int bot[n][m])
{
    int i,j,red,kolona,q,k,x;
    for(q=0;q<4;q++)
    {
        x=0;
        for(i=1;i<11;i++)
        {
            for(j=1;j<11;j++)
            {
                if(bot[i-1][j-1]==88 && bot[i-1][j]==88 && bot[i-1][j+1]==88 && bot[i][j-1]==88 && bot[i][j]==88 && bot[i][j+1]==88 && bot[i+1][j-1]==88 && bot[i+1][j]==88 && bot[i+1][j+1]==88)
                {
                    bot[i][j]=17;
                    x++;
                    break;
                }
            }
            if(x==1)
            {
                break;
            }
        }
    }
}
void bot_brod_2(int n,int m,int bot[n][m])
{
    int i,j,red,kolona,q,k;
    srand(time(NULL));
    i=rand()%2;
    for(q=0;q<3;q++)
    {
        if(i==0)//vodoravno
        {
            j=rand()%2;
            if(j==0)//krece od leva
            {
                a: 
                red=rand()%10+1;
                kolona=rand()%10+1;
                if(bot[red][kolona]==254 || bot[red][kolona]==17 || bot[red][kolona]==30)
                {
                    goto a;
                }
                if(kolona+1<11)
                {
                    k=254;
                    if(bot[red-1][kolona-1]!=254 && bot[red-1][kolona]!=254 && bot[red-1][kolona+1]!=254 && bot[red-1][kolona+2]!=254&&  bot[red][kolona-1]!=254 && bot[red][kolona+2]!=254 && bot[red+1][kolona-1]!=254 && bot[red+1][kolona]!=254 && bot[red+1][kolona+1]!=254 && bot[red+1][kolona+2]!=254)
                    {
                        k=17;
                        if(bot[red-1][kolona-1]!=17 && bot[red-1][kolona]!=17 && bot[red-1][kolona+1]!=17 && bot[red-1][kolona+2]!=17 &&  bot[red][kolona-1]!=17 && bot[red][kolona+2]!=17 && bot[red+1][kolona-1]!=17 && bot[red+1][kolona]!=17 && bot[red+1][kolona+1]!=17 && bot[red+1][kolona+2]!=17)
                        {
                            k=30;
                            if(bot[red-1][kolona-1]!=30 && bot[red-1][kolona]!=30 && bot[red-1][kolona+1]!=30 && bot[red-1][kolona+2]!=30 &&  bot[red][kolona-1]!=30 && bot[red][kolona+2]!=30 && bot[red+1][kolona-1]!=30 && bot[red+1][kolona]!=30 && bot[red+1][kolona+1]!=30 && bot[red+1][kolona+2]!=30)
                            {
                                bot[red][kolona]=17;
                                bot[red][kolona+1]=254;
                            }
                            else
                            {
                                goto a;
                            }
                        }
                        else
                        {
                            goto a;
                        }
                    }
                    else
                    {
                        goto a;
                    }
                }
                else
                {
                    goto a;
                }
            }
            else//krece od desna
            {
                b:
                red=rand()%10+1;
                kolona=rand()%10+1;
                if(bot[red][kolona]==254 || bot[red][kolona]==17 || bot[red][kolona]==30)
                {
                    goto b;
                }
                if(kolona-1>0 && kolona-2>0)
                {
                    k=254;
                    if(bot[red-1][kolona-2]!=254 && bot[red-1][kolona-1]!=254 && bot[red-1][kolona]!=254 && bot[red-1][kolona+1]!=254 &&  bot[red][kolona+1]!=254 && bot[red][kolona-2]!=254 && bot[red+1][kolona-2]!=254 && bot[red+1][kolona-1]!=254 && bot[red+1][kolona]!=254 && bot[red+1][kolona+1]!=254)
                    {
                        k=17;
                        if(bot[red-1][kolona-2]!=17 && bot[red-1][kolona-1]!=17 && bot[red-1][kolona]!=17 && bot[red-1][kolona+1]!=17 &&  bot[red][kolona+1]!=17 && bot[red][kolona-2]!=17 && bot[red+1][kolona-2]!=17 && bot[red+1][kolona-1]!=17 && bot[red+1][kolona]!=17 && bot[red+1][kolona+1]!=17)
                        {
                            k=30;
                            if(bot[red-1][kolona-2]!=30 && bot[red-1][kolona-1]!=30 && bot[red-1][kolona]!=30 && bot[red-1][kolona+1]!=30 &&  bot[red][kolona+1]!=30 && bot[red][kolona-2]!=30 && bot[red+1][kolona-2]!=30 && bot[red+1][kolona-1]!=30 && bot[red+1][kolona]!=30 && bot[red+1][kolona+1]!=30)
                            {
                                bot[red][kolona-1]=17;
                                bot[red][kolona]=254;
                            }
                            else
                            {
                                goto b;
                            }
                        }
                        else
                        {
                            goto b;
                        }
                    }
                    else
                    {
                        goto b;
                    }
                }
                else
                {
                    goto b;
                }
            }
        }
        else//vertikalno
        {
            j=rand()%2;
            if(j==0)//krece od dozgo
            {
                c:
                red=rand()%10+1;
                kolona=rand()%10+1;
                if(bot[red][kolona]==254 || bot[red][kolona]==17 || bot[red][kolona]==30)
                {
                    goto c;
                }
                if(red+1<11 && red+2<11)
                {
                    k=254;
                    if(bot[red-1][kolona-1]!=254 && bot[red][kolona-1]!=254 && bot[red+1][kolona-1]!=254 && bot[red+2][kolona-1]!=254 && bot[red-1][kolona]!=254 && bot[red+2][kolona]!=254 && bot[red-1][kolona+1]!=254 && bot[red][kolona+1]!=254 && bot[red+1][kolona+1]!=254 && bot[red+2][kolona+1]!=254)
                    {
                        k=17;
                        if(bot[red-1][kolona-1]!=17 && bot[red][kolona-1]!=17 && bot[red+1][kolona-1]!=17 && bot[red+2][kolona-1]!=17 && bot[red-1][kolona]!=17 && bot[red+2][kolona]!=17 && bot[red-1][kolona+1]!=17 && bot[red][kolona+1]!=17 && bot[red+1][kolona+1]!=17 && bot[red+2][kolona+1]!=17)
                        {
                            k=30;
                            if(bot[red-1][kolona-1]!=30 && bot[red][kolona-1]!=30 && bot[red+1][kolona-1]!=30 && bot[red+2][kolona-1]!=30 && bot[red-1][kolona]!=30 && bot[red+2][kolona]!=30 && bot[red-1][kolona+1]!=30 && bot[red][kolona+1]!=30 && bot[red+1][kolona+1]!=30 && bot[red+2][kolona+1]!=30)
                            {
                                bot[red][kolona]=30;
                                bot[red+1][kolona]=254;
                            }
                            else
                            {
                                goto c;
                            }
                        }
                        else
                        {
                            goto c;
                        }
                    }
                    else
                    {
                        goto c;
                    }
                }
                else
                {
                    goto c;
                }
            }
            else//krece od dole
            {
                d:
                red=rand()%10+1;
                kolona=rand()%10+1;
                if(bot[red][kolona]==254 || bot[red][kolona]==17 || bot[red][kolona]==30)
                {
                    goto d;
                }
                if(red-1>0 && red-2>0)
                {
                    k=254;
                    if(bot[red-2][kolona-1]!=254 && bot[red-1][kolona-1]!=254 && bot[red][kolona-1]!=254 && bot[red+1][kolona-1]!=254 && bot[red+1][kolona]!=254 && bot[red-2][kolona]!=254&& bot[red-2][kolona+1]!=254 && bot[red-1][kolona+1]!=254 && bot[red][kolona+1]!=254 && bot[red+1][kolona+1]!=254)
                    {
                        k=17;
                        if(bot[red-2][kolona-1]!=17 && bot[red-1][kolona-1]!=17 && bot[red][kolona-1]!=17 && bot[red+1][kolona-1]!=17 && bot[red+1][kolona]!=17 && bot[red-2][kolona]!=17 && bot[red-2][kolona+1]!=17 && bot[red-1][kolona+1]!=17 && bot[red][kolona+1]!=17 && bot[red+1][kolona+1]!=17)
                        {
                            k=30;
                            if(bot[red-2][kolona-1]!=30 && bot[red-1][kolona-1]!=30 && bot[red][kolona-1]!=30 && bot[red+1][kolona-1]!=30 && bot[red+1][kolona]!=30 && bot[red-2][kolona]!=30 && bot[red-2][kolona+1]!=30 && bot[red-1][kolona+1]!=30 && bot[red][kolona+1]!=30 && bot[red+1][kolona+1]!=30)
                            {
                                bot[red-1][kolona]=30;
                                bot[red][kolona]=254;
                            }
                            else
                            {
                                goto d;
                            }
                        }
                        else
                        {
                            goto d;
                        }
                    }
                    else
                    {
                        goto d;
                    }
                }
                else
                {
                    goto d;
                }
            }
        }
        if(i==0)
        {
            i++;
        }
        else
        {
            i--;
        }
    }
}
void bot_brod_3(int n,int m,int bot[n][m])
{
    int i,j,red,kolona,k,q;
    srand(time(NULL));
    i=rand()%2;
    for(q=0;q<2;q++)
    {
        if(i==0)//vodoravno
        {
            j=rand()%2;
            if(j==0)//krece od leva
            {
                a: 
                red=rand()%10+1;
                kolona=rand()%10+1;
                if(bot[red][kolona]==254 || bot[red][kolona]==17 || bot[red][kolona]==30)
                {
                    goto a;
                }
                if(kolona+1<11 && kolona+2<11)
                {
                    k=254;
                    if(bot[red-1][kolona-1]!=254 && bot[red-1][kolona]!=254 && bot[red-1][kolona+1]!=254 && bot[red-1][kolona+2]!=254 && bot[red-1][kolona+3]!=254 &&  bot[red][kolona-1]!=254 && bot[red][kolona+3]!=254 && bot[red+1][kolona-1]!=254 && bot[red+1][kolona]!=254 && bot[red+1][kolona+1]!=254 && bot[red+1][kolona+2]!=254 && bot[red+1][kolona+3]!=254)
                    {
                        k=17;
                        if(bot[red-1][kolona-1]!=17 && bot[red-1][kolona]!=17 && bot[red-1][kolona+1]!=17 && bot[red-1][kolona+2]!=17 && bot[red-1][kolona+3]!=17 &&  bot[red][kolona-1]!=17 && bot[red][kolona+3]!=17 && bot[red+1][kolona-1]!=17 && bot[red+1][kolona]!=17 && bot[red+1][kolona+1]!=17 && bot[red+1][kolona+2]!=17 && bot[red+1][kolona+3]!=17)
                        {
                            k=30;
                            if(bot[red-1][kolona-1]!=30 && bot[red-1][kolona]!=30 && bot[red-1][kolona+1]!=30 && bot[red-1][kolona+2]!=30 && bot[red-1][kolona+3]!=30 &&  bot[red][kolona-1]!=30 && bot[red][kolona+3]!=30 && bot[red+1][kolona-1]!=30 && bot[red+1][kolona]!=30 && bot[red+1][kolona+1]!=30 && bot[red+1][kolona+2]!=30 && bot[red+1][kolona+3]!=30)
                            {
                                bot[red][kolona]=17;
                                bot[red][kolona+1]=254;
                                bot[red][kolona+2]=254;
                            }
                            else
                            {
                                goto a;
                            }
                        }
                        else
                        {
                            goto a;
                        }
                    }
                    else
                    {
                        goto a;
                    }
                }
                else
                {
                    goto a;
                }
            }
            else//krece od desna
            {
                b:
                red=rand()%10+1;
                kolona=rand()%10+1;
                if(bot[red][kolona]==254 || bot[red][kolona]==17 || bot[red][kolona]==30)
                {
                    goto b;
                }
                if(kolona-1>0 && kolona-2>0)
                {
                    k=254;
                    if(bot[red-1][kolona-3]!=254 && bot[red-1][kolona-2]!=254 && bot[red-1][kolona-1]!=254 && bot[red-1][kolona]!=254 && bot[red-1][kolona+1]!=254 &&  bot[red][kolona+1]!=254 && bot[red][kolona-3]!=254 && bot[red+1][kolona-3]!=254 && bot[red+1][kolona-2]!=254 && bot[red+1][kolona-1]!=254 && bot[red+1][kolona]!=254 && bot[red+1][kolona+1]!=254)
                    {
                        k=17;
                        if(bot[red-1][kolona-3]!=17 && bot[red-1][kolona-2]!=17 && bot[red-1][kolona-1]!=17 && bot[red-1][kolona]!=17 && bot[red-1][kolona+1]!=17 &&  bot[red][kolona+1]!=17 && bot[red][kolona-3]!=17 && bot[red+1][kolona-3]!=17 && bot[red+1][kolona-2]!=17 && bot[red+1][kolona-1]!=17 && bot[red+1][kolona]!=17 && bot[red+1][kolona+1]!=17)
                        {
                            k=30;
                            if(bot[red-1][kolona-3]!=30 && bot[red-1][kolona-2]!=30 && bot[red-1][kolona-1]!=30 && bot[red-1][kolona]!=30 && bot[red-1][kolona+1]!=30 &&  bot[red][kolona+1]!=30 && bot[red][kolona-3]!=30 && bot[red+1][kolona-3]!=30 && bot[red+1][kolona-2]!=30 && bot[red+1][kolona-1]!=30 && bot[red+1][kolona]!=30 && bot[red+1][kolona+1]!=30)
                            {
                                bot[red][kolona-2]=17;
                                bot[red][kolona-1]=254;
                                bot[red][kolona]=254;
                            }
                            else
                            {
                                goto b;
                            }
                        }
                        else
                        {
                            goto b;
                        }
                    }
                    else
                    {
                        goto b;
                    }
                }
                else
                {
                    goto b;
                }
            }
        }
        else//vertikalno
        {
            j=rand()%2;
            if(j==0)//krece od dozgo
            {
                c:
                red=rand()%10+1;
                kolona=rand()%10+1;
                if(bot[red][kolona]==254 || bot[red][kolona]==17 || bot[red][kolona]==30)
                {
                    goto c;
                }
                if(red+1<11 && red+2<11)
                {
                    k=254;
                    if(bot[red-1][kolona-1]!=254 && bot[red][kolona-1]!=254 && bot[red+1][kolona-1]!=254 && bot[red+2][kolona-1]!=254 && bot[red+3][kolona-1]!=254 && bot[red-1][kolona]!=254 && bot[red+3][kolona]!=254 && bot[red-1][kolona+1]!=254 && bot[red][kolona+1]!=254 && bot[red+1][kolona+1]!=254 && bot[red+2][kolona+1]!=254 && bot[red+3][kolona+1]!=254)
                    {
                        k=17;
                        if(bot[red-1][kolona-1]!=17 && bot[red][kolona-1]!=17 && bot[red+1][kolona-1]!=17 && bot[red+2][kolona-1]!=17 && bot[red+3][kolona-1]!=17 && bot[red-1][kolona]!=17 && bot[red+3][kolona]!=17 && bot[red-1][kolona+1]!=17 && bot[red][kolona+1]!=17 && bot[red+1][kolona+1]!=17 && bot[red+2][kolona+1]!=17 && bot[red+3][kolona+1]!=17)
                        {
                            k=30;
                            if(bot[red-1][kolona-1]!=30 && bot[red][kolona-1]!=30 && bot[red+1][kolona-1]!=30 && bot[red+2][kolona-1]!=30 && bot[red+3][kolona-1]!=30 && bot[red-1][kolona]!=30 && bot[red+3][kolona]!=30 && bot[red-1][kolona+1]!=30 && bot[red][kolona+1]!=30 && bot[red+1][kolona+1]!=30 && bot[red+2][kolona+1]!=30 && bot[red+3][kolona+1]!=30)
                            {
                                bot[red][kolona]=30;
                                bot[red+1][kolona]=254;
                                bot[red+2][kolona]=254;
                            }
                            else
                            {
                                goto c;
                            }
                        }
                        else
                        {
                            goto c;
                        }
                    }
                    else
                    {
                        goto c;
                    }
                }
                else
                {
                    goto c;
                }
            }
            else//krece od dole
            {
                d:
                red=rand()%10+1;
                kolona=rand()%10+1;
                if(bot[red][kolona]==254 || bot[red][kolona]==17 || bot[red][kolona]==30)
                {
                    goto d;
                }
                if(red-1>0 && red-2>0)
                {
                    k=254;
                    if(bot[red-3][kolona-1]!=254 && bot[red-2][kolona-1]!=254 && bot[red-1][kolona-1]!=254 && bot[red][kolona-1]!=254 && bot[red+1][kolona-1]!=254 && bot[red+1][kolona]!=254 && bot[red-3][kolona]!=254 && bot[red-3][kolona+1]!=254 && bot[red-2][kolona+1]!=254 && bot[red-1][kolona+1]!=254 && bot[red][kolona+1]!=254 && bot[red+1][kolona+1]!=254)
                    {
                        k=17;
                        if(bot[red-3][kolona-1]!=17 && bot[red-2][kolona-1]!=17 && bot[red-1][kolona-1]!=17 && bot[red][kolona-1]!=17 && bot[red+1][kolona-1]!=17 && bot[red+1][kolona]!=17 && bot[red-3][kolona]!=17 && bot[red-3][kolona+1]!=17 && bot[red-2][kolona+1]!=17 && bot[red-1][kolona+1]!=17 && bot[red][kolona+1]!=17 && bot[red+1][kolona+1]!=17)
                        {
                            k=30;
                            if(bot[red-3][kolona-1]!=30 && bot[red-2][kolona-1]!=30 && bot[red-1][kolona-1]!=30 && bot[red][kolona-1]!=30 && bot[red+1][kolona-1]!=30 && bot[red+1][kolona]!=30 && bot[red-3][kolona]!=30 && bot[red-3][kolona+1]!=30 && bot[red-2][kolona+1]!=30 && bot[red-1][kolona+1]!=30 && bot[red][kolona+1]!=30 && bot[red+1][kolona+1]!=30)
                            {
                                bot[red-2][kolona]=30;
                                bot[red-1][kolona]=254;
                                bot[red][kolona]=254;
                            }
                            else
                            {
                                goto d;
                            }
                        }
                        else
                        {
                            goto d;
                        }
                    }
                    else
                    {
                        goto d;
                    }
                }
                else
                {
                    goto d;
                }
            }
        }
        if(i==0)
        {
            i++;
        }
        else
        {
            i--;
        }
    }
}   
void bot_brod_4(int n,int m,int bot[n][m])
{
    int i,j,red,kolona;
    srand(time(NULL));
    i=rand()%2;
    if(i==0)//vodoravno
    {
        j=rand()%2;
        if(j==0)//krece od leva
        {
            a:
            red=rand()%10+1;
            kolona=rand()%10+1;
            if(kolona+1<11 && kolona+2<11 && kolona+3<11)
            {
                bot[red][kolona]=17;
                bot[red][kolona+1]=254;
                bot[red][kolona+2]=254;
                bot[red][kolona+3]=254;
            }
            else
            {
                goto a;
            }
        }
        else//krece od desna
        {
            b:
            red=rand()%10+1;
            kolona=rand()%10+1;
            if(kolona-1>0 && kolona-2>0 && kolona-3>0)
            {
                bot[red][kolona-3]=17;
                bot[red][kolona-2]=254;
                bot[red][kolona-1]=254;
                bot[red][kolona]=254;
            }
            else
            {
                goto b;
            }
        }
    }
    else//vertikalno
    {
        j=rand()%2;
        if(j==0)//krece od dozgo
        {
            c:
            red=rand()%10+1;
            kolona=rand()%10+1;
            if(red+1<11 && red+2<11 && red+3<11)
            {
                bot[red][kolona]=30;
                bot[red+1][kolona]=254;
                bot[red+2][kolona]=254;
                bot[red+3][kolona]=254;
            }
            else
            {
                goto c;
            }
        }
        else//krece od dole
        {
            d:
            red=rand()%10+1;
            kolona=rand()%10+1;
            if(red-1>0 && red-2>0 && red-3>0)
            {
                bot[red-3][kolona]=30;
                bot[red-2][kolona]=254;
                bot[red-1][kolona]=254;
                bot[red][kolona]=254;
            }
            else
            {
                goto d;
            }
        }
    }
}
