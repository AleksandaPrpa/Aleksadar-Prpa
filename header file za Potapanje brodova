void kontrolna_tabla()
{
    int c;
    c=1;
    if(c==1)
    {
        gamamode();
    }
    else
    {
        if(c==2)
        {
            podesavanja();
        }
        else
        {
            system("cls");
            printf("Pogresan unos.\n");
        }
    }
}
void gamamode()
{
    int c;
    a:
    system("cls");
    printf("1.Klasicno\n2.Mod\n");
    scanf("%d",&c);
    if(c==1)
    {
        Klasicno();
    }
    else
    {
        if(c==2)
        {
            Mod();
        }
        else
        {
            system("cls");
            printf("Pogresan unos.\n");
            goto a;
        }
    }
}
void podesavanja()
{}
void Klasicno()
{
    int c;
    a:
    system("cls");
    printf("1.Za dva igraca\n2.Protiv kompjutera\n3.Nazad\n");
    scanf("%d",&c);
    switch (c)
    {
    case 1:
        multiplayer();
        break;
    case 2:
        bot();
        break;
    case 3:
        gamamode();
        break;
    default:
        system("cls");
        printf("Pogresan unos.\n");
        goto a;
        break;
    }
    
}
void unos_naoruzanja(int n,int m,int prvi_igrac[n][m],int l,int prvi_igrac_naoruzanje[l])
{
    int novac=260,x;
    int borac=0,borac_o=2,nosac_torpeda=0,nosac_torpeda_o=2,bombarder=0,bombarder_o=2,atomski_bombarder=0,atomski_bombarder_o=1,odbrana=0,odbrana_o=3,radar=0,radar_o=1,podmornica=0,podmornica_o=1;   
    int r1,r2,p,q,y,z;
    while(novac>0)
    {
        printf("Unesi redni broj da dodas naoruzanje\nImas %d novca\n",novac);
        printf("-1. Borac-stanje %d-kosta 15 dinara\n\tX X X X X X X X X X\n\tX O O O O O X X X X\n\tX O O O O O X X X X\n\tX X X X X X X X X X\n",borac_o);
        printf("-2. Nosac torpeda-stanje %d-kosta 10 dinara\n\tX X X X X X X X X X\n\tO O O O O O O O O O\n\tX X X X X X X X X X\n",nosac_torpeda_o);
        printf("-3. Bombarder-stanje %d-kosta 12 dinara\n\tX X X X X X X X X X\n\tX X X X O X X X X X\n\tX X X X O O X X X X\n\tX X X X X X O X X X\n\tX X X X X X X X X X\n\tNasumecno baci cetiri bombe.\n",bombarder_o);
        printf("-4. Atomski bombarder-stanje %d-kosta 100 dinara\n\tX X X X X X X X X X\n\tX O O O O O X X X X\n\tX O O O O O X X X X\n\tX O O O O O X X X X\n\tX O O O O O X X X X\n\tX O O O O O X X X X\n\tX X X X X X X X X X\n",atomski_bombarder_o);
        printf("-5. Odbrana-stanje %d-kosta 10 dinara\n\tO O O O O O O O O O\n\tO O O O O O O O O O\n",odbrana_o);
        printf("-6. Radar-stanje %d-kosta 15 dinara\n\tX X X X X X X X X X\n\tX X X X O O O X X X\n\tX X X X O O O X X X\n\tX X X X O O O X X X\n\tX X X X X X X X X X\n\tIspita da li ima protivnickog broda.\n",radar_o);
        printf("-7. Podmornica-stanje %d-kosta 10 dinara\n\tX X X O X X X X X X\n\tX X X O X X X X X X\n\tX X X O X X X X X X\n\tX X X O X X X X X X\n\tX O O O X X X X X X\n\tX X O X X X X X X X\n\tX X O X X X X X X X\n\tX X O X X X X X X X\n\tX X O X X X X X X X\n\tX X O X X X X X X X\n",podmornica_o);
        printf("-8. Nastavi\n");
        scanf("%d",&x);
        switch (x)
        {
        case 1:
            system("cls");
            if(borac_o!=0)
            {
                borac++;
                borac_o--;
                novac=novac-15;
            }
            else
            {
                printf("Vise nema na stanju");
            }
            break;
        case 2:
            system("cls");
            if(nosac_torpeda_o!=0)
            {
                nosac_torpeda++;
                nosac_torpeda_o--;
                novac=novac-10;
            }
            else
            {
                printf("Vise nema na stanju");
            }
            break;
        case 3:
            system("cls");
            if(bombarder_o!=0)
            {
                bombarder++;
                bombarder_o--;
                novac=novac-12;
            }
            else
            {
                printf("Vise nema na stanju");
            }
            break;
        case 4:
            system("cls");
            if(atomski_bombarder_o!=0)
            {
                atomski_bombarder++;
                atomski_bombarder_o--;
                novac=novac-100;
            }
            else
            {
                printf("Vise nema na stanju");
            }
            break;
        case 5:       
            if(odbrana_o!=0)
            {
                odbrana++;
                odbrana_o--;
                novac=novac-10;
                a:
                getch();
                system("cls"); 
                if(odbrana==1)
                {
                    printf("Unesi koja dva reda hoces da odbranis od aviona.\n");
                    scanf("%d%d",&r1,&r2);
                    if(r1<1 || r1>10)
                    {
                        printf("Pogresan unos.\n");
                        goto a;
                    }
                    else
                    {
                        if(r2<1 || r2>10)
                        {
                            printf("Pogresan unos.\n");
                            goto a;
                        } 
                    }
                    if(r1==r2)
                    {
                        printf("Pogresan unos.\nMoraju da budu razliciti redovi.\n");
                        goto a;
                    }
                    if(abs(r1-r2)!=1)
                    {
                        printf("Pogresan unos.\nTreba da budu jedan do drugog.\n");
                        goto a;
                    }
                    if(prvi_igrac[r1][0]!=1 && prvi_igrac[r2][0]!=1)
                    {
                        printf("Pogresan unos.\nVec postoji odbrana tu.\n");
                    }
                    prvi_igrac[r1][0]=1;
                    prvi_igrac[r2][0]=1;
                }
                if(odbrana==2)
                {
                    printf("Unesi koja dva reda hoces da odbranis od aviona.\n");
                    scanf("%d%d",&p,&q);
                    if(p==q)
                    {
                        printf("Pogresan unos.\nMoraju da budu razliciti redovi.\n");
                        goto a;
                    }
                    if(abs(p-q)!=1)
                    {
                        printf("Pogresan unos.\nTreba da budu jedan do drugog.\n");
                        goto a;
                    }
                    if(p==r1 || p==r2 || q==r1 || q==r2)
                    {
                        printf("Pogresan unos.\nOdbrana se preklapa.\n");
                        goto a;
                    }
                    
                    prvi_igrac[p][0]=1;
                    prvi_igrac[q][0]=1;
                }
                if(odbrana==3)
                {
                    printf("Unesi koja dva reda hoces da odbranis od aviona.\n");
                    scanf("%d%d",&y,&z);
                    if(y==z)
                    {
                        printf("Pogresan unos.\nMoraju da budu razliciti redovi.\n");
                        goto a;
                    }
                    if(abs(z-y)!=1)
                    {
                        printf("Pogresan unos.\nTreba da budu jedan do drugog.\n");
                        goto a;
                    }
                    if(y==r1 || y==r2 || z==r1 || z==r2 || y==p || y==q || z==p || z==q)
                    {
                        printf("Pogresan unos.\nOdbrana se preklapa.\n");
                        goto a;
                    }
                    prvi_igrac[y][0]=1;
                    prvi_igrac[z][0]=1;
                }
            }
            else
            {
                printf("Vise nema na stanju");
            }
            break;
        case 6:
            system("cls");
            if(radar_o!=0)
            {
                radar++;
                radar_o--;
                novac=novac-15;
            }
            else
            {
                printf("Vise nema na stanju");
            }
            break;
        case 7:
            system("cls");
            if(podmornica_o!=0)
            {
                podmornica++;
                podmornica_o--;
                novac=novac-10;
            }
            else
            {
                printf("Vise nema na stanju");
            }
            break;
        case 8:
            system("cls");
            novac=0;
            break;
        default:
            system("cls");
            printf("Pogresan unos");
            break;
        }
        printf("\n");
    }
    prvi_igrac_naoruzanje[0]=borac;
    prvi_igrac_naoruzanje[1]=nosac_torpeda;
    prvi_igrac_naoruzanje[2]=bombarder;
    prvi_igrac_naoruzanje[3]=atomski_bombarder;
    prvi_igrac_naoruzanje[4]=odbrana;
    prvi_igrac_naoruzanje[5]=radar;
    prvi_igrac_naoruzanje[6]=podmornica;
}
void Mod()
{
    int c;
    a:
    system("cls");
    printf("1.Za dva igraca\n2.Protiv kompjutera\n3.Nazad\n");
    scanf("%d",&c);
    switch (c)
    {
    case 1:
        multiplayer_mod();
        break;
    case 2:
        bot_mod();
        break;
    case 3:
        gamamode();
        break;
    default:
        system("cls");
        printf("Pogresan unos.\n");
        goto a;
        break;
    }
}
void multiplayer()
{
    pravila1();
    getch();
    system("cls");
    int i, j,y=1; // brojaci  y-brojac broda
    int n=12,m=12;
    int potez = 0, rezultat = 0,potop1=0,potop2=0;
    int red1,kolona1;
    int prvi_igrac[n][m];
    int drugi_igrac[n][m];
    int prvi_igrac_sakriveno[n][m];
    int drugi_igrac_sakriveno[n][m];
    char ime_prvi_igrac[100];
    char ime_drugi_igrac[100];
    printf("Unesi ime prvog igraca: ");
    gets(ime_prvi_igrac);
    gets(ime_prvi_igrac);
    printf("\nUnesi ime drugog igraca: ");
    gets(ime_drugi_igrac);
    system("cls");
    for(i=0;i<12;i++)
    {
        for(j=0;j<12;j++)
        {
            prvi_igrac[i][j]=88;//
            drugi_igrac[i][j]=88;
            prvi_igrac_sakriveno[i][j]=88;
            drugi_igrac_sakriveno[i][j]=88;
        }
    }
    printf("%s namesta brodove\n", ime_prvi_igrac);
    ispis_table();
    unos_brodova(n,m,prvi_igrac);
    printf("Pritisni bilo koje dugme da sledeci igrac namesta brodove.\n");
    getch();
    system("cls");
    printf("%s namesta brodove\n", ime_drugi_igrac);
    ispis_table();
    unos_brodova(n,m,drugi_igrac);
    printf("Pritisni bilo koje dugme da nastavis.\n");
    getch();
	while (rezultat == 0)
    {
        if (potez % 2 == 0)
        {
            system("cls");
            isaisdas(n,m,drugi_igrac_sakriveno);
            printf("%s igra\n", ime_prvi_igrac);
            switch (provera(n,m,drugi_igrac,drugi_igrac_sakriveno))
            {
            case 0:
                system("cls");
                isaisdas(n,m,drugi_igrac_sakriveno);
                printf("Promasaj\n");
                getch();
                system("cls");
                break;
            case 1:
                potez++;
                system("cls");
                isaisdas(n,m,drugi_igrac_sakriveno);
                getch();
                system("cls");
                break;
            case 2:
                potez++;
                potop1++;
                system("cls");
                isaisdas(n,m,drugi_igrac_sakriveno);
                getch();
                system("cls");
                break;
            default:
                break;
            }
            if(potop1==10)
            {
                rezultat++;
                printf("%s je pobednik\nPostava brodova od %s je\n", ime_prvi_igrac,ime_prvi_igrac);
                isaisdas(n,m,prvi_igrac);
            }
        }
        else
        {   
            system("cls");
            isaisdas(n,m,prvi_igrac_sakriveno);
            printf("%s igra\n", ime_drugi_igrac);
            switch (provera(n,m,prvi_igrac,prvi_igrac_sakriveno))
            {
            case 0:
                system("cls");
                isaisdas(n,m,prvi_igrac_sakriveno);
                printf("Promasaj\n");
                getch();
                system("cls");
                break;
            case 1:
                potez++;
                system("cls");
                isaisdas(n,m,prvi_igrac_sakriveno);
                getch();
                system("cls");
                break;
            case 2:
                potez++;
                potop2++;
                system("cls");
                isaisdas(n,m,prvi_igrac_sakriveno);
                getch();
                system("cls");
                break;
            default:
                break;
            }
            if(potop2==10)
            {
                rezultat++;
                printf("%s je pobednik\nPostava brodova od %s je\n", ime_drugi_igrac,ime_drugi_igrac);
                isaisdas(n,m,drugi_igrac);
            }
        }
       potez++;
    }
}
int provera(int n,int m,int drugi_igrac[n][m],int drugi_igrac_sakriveno[n][m])
{
    int y,potop1,red1,kolona1,potez;
    p:
    red1=unos_polja();
    kolona1=unos_polja();
    if(drugi_igrac[red1][kolona1]==79 || drugi_igrac[red1][kolona1]==335)
    {
        system("cls");
        isaisdas(n,m,drugi_igrac_sakriveno);
        printf("Igrac igra\n");
        printf("Vec si gadjao to polje\n");
        goto p;
    }
    if(drugi_igrac[red1][kolona1]!=88)
    {
                system("cls");
                printf("Pogodak\n");
                potez++;
                drugi_igrac_sakriveno[red1][kolona1]=79;
                drugi_igrac[red1][kolona1]=335;
                if(drugi_igrac[red1-1][kolona1-1]!=30 && drugi_igrac[red1-1][kolona1]!=30 && drugi_igrac[red1-1][kolona1+1]!=30 && drugi_igrac[red1][kolona1-1]!=30 && drugi_igrac[red1][kolona1+1]!=30 && drugi_igrac[red1+1][kolona1-1]!=30 && drugi_igrac[red1+1][kolona1]!=30 && drugi_igrac[red1+1][kolona1+1]!=30)
                {
                    if(drugi_igrac[red1-1][kolona1-1]!=17 && drugi_igrac[red1-1][kolona1]!=17 && drugi_igrac[red1-1][kolona1+1]!=17 && drugi_igrac[red1][kolona1-1]!=17 && drugi_igrac[red1][kolona1+1]!=17 && drugi_igrac[red1+1][kolona1-1]!=17 && drugi_igrac[red1+1][kolona1]!=17 && drugi_igrac[red1+1][kolona1+1]!=17)
                    {
                        if(drugi_igrac[red1-1][kolona1-1]!=254 && drugi_igrac[red1-1][kolona1]!=254 && drugi_igrac[red1-1][kolona1+1]!=254 && drugi_igrac[red1][kolona1-1]!=254 && drugi_igrac[red1][kolona1+1]!=254 && drugi_igrac[red1+1][kolona1-1]!=254 && drugi_igrac[red1+1][kolona1]!=254 && drugi_igrac[red1+1][kolona1+1]!=254)
                        {
                            y=1;
                            system("cls");
                            potop1++;
                            printf("Potop\n");
                            if(drugi_igrac[red1][kolona1-1]==335 || drugi_igrac[red1][kolona1+1]==335)
                            {
                                if(drugi_igrac[red1][kolona1-1]==335)
                                {
                                    y++;
                                    if(drugi_igrac[red1][kolona1-2]==335)
                                    {
                                        y++;
                                        if(drugi_igrac[red1][kolona1-3]==335)
                                        {
                                            y++;
                                            drugi_igrac[red1+1][kolona1-4]=79;
                                            drugi_igrac[red1+1][kolona1-3]=79;
                                            drugi_igrac[red1+1][kolona1-2]=79;
                                            drugi_igrac[red1+1][kolona1-1]=79;
                                            drugi_igrac[red1+1][kolona1]=79;
                                            drugi_igrac[red1+1][kolona1+1]=79;

                                            drugi_igrac[red1][kolona1-4]=79;
                                            drugi_igrac[red1][kolona1+1]=79;

                                            drugi_igrac[red1-1][kolona1-4]=79;
                                            drugi_igrac[red1-1][kolona1-3]=79;
                                            drugi_igrac[red1-1][kolona1-2]=79;
                                            drugi_igrac[red1-1][kolona1-1]=79;
                                            drugi_igrac[red1-1][kolona1]=79;
                                            drugi_igrac[red1-1][kolona1+1]=79;
                                            
                                            drugi_igrac_sakriveno[red1+1][kolona1-4]=79;
                                            drugi_igrac_sakriveno[red1+1][kolona1-3]=79;
                                            drugi_igrac_sakriveno[red1+1][kolona1-2]=79;
                                            drugi_igrac_sakriveno[red1+1][kolona1-1]=79;
                                            drugi_igrac_sakriveno[red1+1][kolona1]=79;
                                            drugi_igrac_sakriveno[red1+1][kolona1+1]=79;

                                            drugi_igrac_sakriveno[red1][kolona1-4]=79;
                                            drugi_igrac_sakriveno[red1][kolona1+1]=79;

                                            drugi_igrac_sakriveno[red1-1][kolona1-4]=79;
                                            drugi_igrac_sakriveno[red1-1][kolona1-3]=79;
                                            drugi_igrac_sakriveno[red1-1][kolona1-2]=79;
                                            drugi_igrac_sakriveno[red1-1][kolona1-1]=79;
                                            drugi_igrac_sakriveno[red1-1][kolona1]=79;
                                            drugi_igrac_sakriveno[red1-1][kolona1+1]=79;
                                        }
                                        else
                                        {
                                            if(drugi_igrac[red1][kolona1+1]==335)
                                            {
                                                y++;
                                                drugi_igrac[red1+1][kolona1-3]=79;
                                                drugi_igrac[red1+1][kolona1-2]=79;
                                                drugi_igrac[red1+1][kolona1-1]=79;
                                                drugi_igrac[red1+1][kolona1]=79;
                                                drugi_igrac[red1+1][kolona1+1]=79;
                                                drugi_igrac[red1+1][kolona1+2]=79;

                                                drugi_igrac[red1][kolona1-3]=79;
                                                drugi_igrac[red1][kolona1+2]=79;

                                                drugi_igrac[red1-1][kolona1-3]=79;
                                                drugi_igrac[red1-1][kolona1-2]=79;
                                                drugi_igrac[red1-1][kolona1-1]=79;
                                                drugi_igrac[red1-1][kolona1]=79;
                                                drugi_igrac[red1-1][kolona1+1]=79;
                                                drugi_igrac[red1-1][kolona1+2]=79;

                                                drugi_igrac_sakriveno[red1+1][kolona1-3]=79;
                                                drugi_igrac_sakriveno[red1+1][kolona1-2]=79;
                                                drugi_igrac_sakriveno[red1+1][kolona1-1]=79;
                                                drugi_igrac_sakriveno[red1+1][kolona1]=79;
                                                drugi_igrac_sakriveno[red1+1][kolona1+1]=79;
                                                drugi_igrac_sakriveno[red1+1][kolona1+2]=79;

                                                drugi_igrac_sakriveno[red1][kolona1-3]=79;
                                                drugi_igrac_sakriveno[red1][kolona1+2]=79;

                                                drugi_igrac_sakriveno[red1-1][kolona1-3]=79;
                                                drugi_igrac_sakriveno[red1-1][kolona1-2]=79;
                                                drugi_igrac_sakriveno[red1-1][kolona1-1]=79;
                                                drugi_igrac_sakriveno[red1-1][kolona1]=79;
                                                drugi_igrac_sakriveno[red1-1][kolona1+1]=79;
                                                drugi_igrac_sakriveno[red1-1][kolona1+2]=79;
                                            }
                                            else
                                            {
                                                drugi_igrac[red1+1][kolona1-3]=79;
                                                drugi_igrac[red1+1][kolona1-2]=79;
                                                drugi_igrac[red1+1][kolona1-1]=79;
                                                drugi_igrac[red1+1][kolona1]=79;
                                                drugi_igrac[red1+1][kolona1+1]=79;

                                                drugi_igrac[red1][kolona1-3]=79;
                                                drugi_igrac[red1][kolona1+1]=79;

                                                drugi_igrac[red1-1][kolona1-3]=79;
                                                drugi_igrac[red1-1][kolona1-2]=79;
                                                drugi_igrac[red1-1][kolona1-1]=79;
                                                drugi_igrac[red1-1][kolona1]=79;
                                                drugi_igrac[red1-1][kolona1+1]=79;

                                                drugi_igrac_sakriveno[red1+1][kolona1-3]=79;
                                                drugi_igrac_sakriveno[red1+1][kolona1-2]=79;
                                                drugi_igrac_sakriveno[red1+1][kolona1-1]=79;
                                                drugi_igrac_sakriveno[red1+1][kolona1]=79;
                                                drugi_igrac_sakriveno[red1+1][kolona1+1]=79;

                                                drugi_igrac_sakriveno[red1][kolona1-3]=79;
                                                drugi_igrac_sakriveno[red1][kolona1+1]=79;

                                                drugi_igrac_sakriveno[red1-1][kolona1-3]=79;
                                                drugi_igrac_sakriveno[red1-1][kolona1-2]=79;
                                                drugi_igrac_sakriveno[red1-1][kolona1-1]=79;
                                                drugi_igrac_sakriveno[red1-1][kolona1]=79;
                                                drugi_igrac_sakriveno[red1-1][kolona1+1]=79;
                                            }
                                        }
                                    }
                                    else
                                    {
                                       if(drugi_igrac[red1][kolona1+1]==335)
                                        {
                                            y++;
                                            if(drugi_igrac[red1][kolona1+2]==335)
                                            {
                                                y++;
