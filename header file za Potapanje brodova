                                {
                                    if(prvi_igrac[red1-1][kolona1]!=17 && prvi_igrac[red1][kolona1]!=17 && prvi_igrac[red1-1][kolona1]!=30 && prvi_igrac[red1][kolona1]!=30)
                                    {
