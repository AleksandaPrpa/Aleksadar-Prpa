                                                    drugi_igrac[red1-1][kolona1+1]=79;
                                                    drugi_igrac[red1][kolona1+1]=79;
                                                    drugi_igrac[red1+1][kolona1+1]=79;
                                                    drugi_igrac[red1+2][kolona1+1]=79;
                                                    drugi_igrac[red1+3][kolona1+1]=79;

                                                    drugi_igrac_sakriveno[red1-1][kolona1-1]=79;
                                                    drugi_igrac_sakriveno[red1][kolona1-1]=79;
                                                    drugi_igrac_sakriveno[red1+1][kolona1-1]=79;
                                                    drugi_igrac_sakriveno[red1+2][kolona1-1]=79;
                                                    drugi_igrac_sakriveno[red1+3][kolona1-1]=79;

                                                    drugi_igrac_sakriveno[red1-1][kolona1]=79;
                                                    drugi_igrac_sakriveno[red1+3][kolona1]=79;

                                                    drugi_igrac_sakriveno[red1-1][kolona1+1]=79;
                                                    drugi_igrac_sakriveno[red1][kolona1+1]=79;
                                                    drugi_igrac_sakriveno[red1+1][kolona1+1]=79;
                                                    drugi_igrac_sakriveno[red1+2][kolona1+1]=79;
                                                    drugi_igrac_sakriveno[red1+3][kolona1+1]=79;
                                                }
                                            }
                                            else
                                            {
                                                drugi_igrac[red1-1][kolona1-1]=79;
                                                drugi_igrac[red1][kolona1-1]=79;
                                                drugi_igrac[red1+1][kolona1-1]=79;
                                                drugi_igrac[red1+2][kolona1-1]=79;

                                                drugi_igrac[red1-1][kolona1]=79;
                                                drugi_igrac[red1+2][kolona1]=79;

                                                drugi_igrac[red1-1][kolona1+1]=79;
                                                drugi_igrac[red1][kolona1+1]=79;
                                                drugi_igrac[red1+1][kolona1+1]=79;
                                                drugi_igrac[red1+2][kolona1+1]=79;

                                                drugi_igrac_sakriveno[red1-1][kolona1-1]=79;
                                                drugi_igrac_sakriveno[red1][kolona1-1]=79;
                                                drugi_igrac_sakriveno[red1+1][kolona1-1]=79;
                                                drugi_igrac_sakriveno[red1+2][kolona1-1]=79;

                                                drugi_igrac_sakriveno[red1-1][kolona1]=79;
                                                drugi_igrac_sakriveno[red1+2][kolona1]=79;

                                                drugi_igrac_sakriveno[red1-1][kolona1+1]=79;
                                                drugi_igrac_sakriveno[red1][kolona1+1]=79;
                                                drugi_igrac_sakriveno[red1+1][kolona1+1]=79;
                                                drugi_igrac_sakriveno[red1+2][kolona1+1]=79;
                                            }
                                        }
                                        else
                                        {
                                            drugi_igrac[red1+1][kolona1+1]=79;
                                            drugi_igrac[red1+1][kolona1]=79;
                                            drugi_igrac[red1+1][kolona1-1]=79;

                                            drugi_igrac[red1][kolona1+1]=79;
                                            drugi_igrac[red1][kolona1-1]=79;

                                            drugi_igrac[red1-1][kolona1+1]=79;
                                            drugi_igrac[red1-1][kolona1]=79;
                                            drugi_igrac[red1-1][kolona1-1]=79;

                                            drugi_igrac_sakriveno[red1+1][kolona1+1]=79;
                                            drugi_igrac_sakriveno[red1+1][kolona1]=79;
                                            drugi_igrac_sakriveno[red1+1][kolona1-1]=79;

                                            drugi_igrac_sakriveno[red1][kolona1+1]=79;
                                            drugi_igrac_sakriveno[red1][kolona1-1]=79;

                                            drugi_igrac_sakriveno[red1-1][kolona1+1]=79;
                                            drugi_igrac_sakriveno[red1-1][kolona1]=79;
                                            drugi_igrac_sakriveno[red1-1][kolona1-1]=79;
                                        }
                                    }
                                }
                                else
                                {
                                    drugi_igrac[red1+1][kolona1+1]=79;
                                    drugi_igrac[red1+1][kolona1]=79;
                                    drugi_igrac[red1+1][kolona1-1]=79;

                                    drugi_igrac[red1][kolona1+1]=79;
                                    drugi_igrac[red1][kolona1-1]=79;

                                    drugi_igrac[red1-1][kolona1+1]=79;
                                    drugi_igrac[red1-1][kolona1]=79;
                                    drugi_igrac[red1-1][kolona1-1]=79;

                                    drugi_igrac_sakriveno[red1+1][kolona1+1]=79;
                                    drugi_igrac_sakriveno[red1+1][kolona1]=79;
                                    drugi_igrac_sakriveno[red1+1][kolona1-1]=79;

                                    drugi_igrac_sakriveno[red1][kolona1+1]=79;
                                    drugi_igrac_sakriveno[red1][kolona1-1]=79;

                                    drugi_igrac_sakriveno[red1-1][kolona1+1]=79;
                                    drugi_igrac_sakriveno[red1-1][kolona1]=79;
                                    drugi_igrac_sakriveno[red1-1][kolona1-1]=79;
                                }
                            }
                            return 2;
                        }
                        else
                        {
                            return 1;
                        }
                    }
                    else
                    {
                        return 1;
                    }
                }
                else
                {
                    return 1;
                }
    }
    else
    {
        drugi_igrac[red1][kolona1]=79;
        drugi_igrac_sakriveno[red1][kolona1]=79;
        return 0;
    }
}
void pravila1()
{
    printf("Igra se na tabli 10x10.\nIma dva igraca.\nSvako ima da postavi:\n-4 broda duzine 1\n-3 broda duzine 2\n-2 broda duzine 3\n-1 brod duzine duzine 4.\nRazdaljina izmedju brodova mora biti 1.\n");
    printf("Pritisni bilo koje dugme da nastavis.\n");
}
