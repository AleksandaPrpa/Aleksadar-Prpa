                                                drugi_igrac[red1-1][kolona1-2]=79;
                                                drugi_igrac[red1-1][kolona1-1]=79;
                                                drugi_igrac[red1-1][kolona1]=79;
                                                drugi_igrac[red1-1][kolona1+1]=79;
                                                drugi_igrac[red1-1][kolona1+2]=79;
                                                drugi_igrac[red1-1][kolona1+3]=79;

                                                drugi_igrac_sakriveno[red1+1][kolona1-2]=79;
