                                                    drugi_igrac[red1-1][kolona1+1]=79;
                                                    drugi_igrac[red1][kolona1+1]=79;
                                                    drugi_igrac[red1+1][kolona1+1]=79;
                                                    drugi_igrac[red1+2][kolona1+1]=79;
                                                    drugi_igrac[red1+3][kolona1+1]=79;
                                                    drugi_igrac[red1+4][kolona1+1]=79;

                                                    drugi_igrac_sakriveno[red1-1][kolona1-1]=79;
                                                    drugi_igrac_sakriveno[red1][kolona1-1]=79;
                                                    drugi_igrac_sakriveno[red1+1][kolona1-1]=79;
                                                    drugi_igrac_sakriveno[red1+2][kolona1-1]=79;
                                                    drugi_igrac_sakriveno[red1+3][kolona1-1]=79;
                                                    drugi_igrac_sakriveno[red1+4][kolona1-1]=79;

                                                    drugi_igrac_sakriveno[red1-1][kolona1]=79;
                                                    drugi_igrac_sakriveno[red1+4][kolona1]=79;

                                                    drugi_igrac_sakriveno[red1-1][kolona1+1]=79;
                                                    drugi_igrac_sakriveno[red1][kolona1+1]=79;
                                                    drugi_igrac_sakriveno[red1+1][kolona1+1]=79;
                                                    drugi_igrac_sakriveno[red1+2][kolona1+1]=79;
                                                    drugi_igrac_sakriveno[red1+3][kolona1+1]=79;
                                                    drugi_igrac_sakriveno[red1+4][kolona1+1]=79;
                                                }
                                                else
                                                {
                                                    drugi_igrac[red1-1][kolona1-1]=79;
                                                    drugi_igrac[red1][kolona1-1]=79;
                                                    drugi_igrac[red1+1][kolona1-1]=79;
                                                    drugi_igrac[red1+2][kolona1-1]=79;
                                                    drugi_igrac[red1+3][kolona1-1]=79;

                                                    drugi_igrac[red1-1][kolona1]=79;
