    {
    	for(br2=0;br2<3;br2++)
    	{
            printf("Brod duzine 2\nUnesi od kog polja do kog polja ce da bude\nPrvo unosis red %c pa kolonu %c\n",281,282);
            d:
            printf("Od\n");
            red1= unos_polja();
            kolona1=unos_polja();
            printf("Do\n");
            red2= unos_polja();
            kolona2=unos_polja();
            if(red1==red2 && kolona1==kolona2)
            {
                printf("To je brod duzine 1.\nUnesi ponovo\n");
                goto d;
            }
            else
            {
                if(red1==red2 || kolona1==kolona2)
                {
                    if(abs(red1-red2)==1 || abs(kolona1-kolona2)==1)
                    {
                        if(red1==red2)
                        {
                            if(kolona1>kolona2)
                            {
                                if(prvi_igrac[red1-1][kolona1-2]==88 && prvi_igrac[red1-1][kolona1-1]==88 && prvi_igrac[red1-1][kolona1]==88 && prvi_igrac[red1-1][kolona1+1]==88 && prvi_igrac[red1][kolona1-2]==88 && prvi_igrac[red1][kolona1+1]==88 && prvi_igrac[red1+1][kolona1-2]==88 && prvi_igrac[red1+1][kolona1-1]==88 && prvi_igrac[red1+1][kolona1]==88 && prvi_igrac[red1+1][kolona1+1]==88)
                                {
                                    if(prvi_igrac[red1][kolona1-1]!=17 && prvi_igrac[red1][kolona1]!=17 && prvi_igrac[red1][kolona1-1]!=30 && prvi_igrac[red1][kolona1]!=30)
                                    {
                                        if(prvi_igrac[red1][kolona1-1]!=254 && prvi_igrac[red1][kolona1]!=254)
                                        {
                                            prvi_igrac[red1][kolona1]=254;
                                            prvi_igrac[red1][kolona1-1]=17;
                                        }
                                        else
                                        {
                                            printf("Ne mogu dva broda da budu na istom polju\n");
                                            goto d;   
                                        }
                                    }
                                    else
                                    {
                                        printf("Ne mogu dva broda da budu na istom polju\n");
                                        goto d;
                                    }
                                }
                                else
                                {
                                    printf("Brodovi moraju da imaju razmak duzine 1.\n");
                                    goto d;
                                }
                            }
                            else
                            {
                                if(prvi_igrac[red1-1][kolona2-2]==88 && prvi_igrac[red1-1][kolona2-1]==88 && prvi_igrac[red1-1][kolona2]==88 && prvi_igrac[red1-1][kolona2+1]==88 && prvi_igrac[red1][kolona2-2]==88 && prvi_igrac[red1][kolona2+1]==88 && prvi_igrac[red1+1][kolona2-2]==88 && prvi_igrac[red1+1][kolona2-1]==88 && prvi_igrac[red1+1][kolona2]==88 && prvi_igrac[red1+1][kolona2+1]==88)
                                {
                                    if(prvi_igrac[red1][kolona2-1]!=17 && prvi_igrac[red1][kolona2]!=17 && prvi_igrac[red1][kolona2-1]!=30 && prvi_igrac[red1][kolona2]!=30)
                                    {
