kraj()
{
    char o[2];
    a:
    getch();
    system("cls");
    printf("Da li zelite jos jednu partiju?\n");
    gets(o);
    if(strcmpi(o,"da")==0)
    {
        kontrolna_tabla();
    }
    else
    {
        if(strcmpi(o,"ne")==0)
        {}
        else
        {
            system("cls");
            printf("Pogresan unos.\n");
            goto a;
        }
    }
}
