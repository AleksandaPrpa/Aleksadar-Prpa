unos_naoruzanja_bot(int n,int m,int bot[n][m],int l,int bot_naoruzanje[l])
{
    int novac=200,x;
    int borac=0,borac_o=2,nosac_torpeda=0,nosac_torpeda_o=2,bombarder=0,bombarder_o=2,atomski_bombarder=0,atomski_bombarder_o=1,odbrana=0,odbrana_o=3,radar=0,radar_o=1,podmornica=0,podmornica_o=1;   
    int r1,r2,p,q,y,z;
    srand(time(NULL));
    x=rand()%4;
    switch (x)
    {
    case 0:
        bot_naoruzanje[0]=2;
        bot_naoruzanje[1]=2;
        bot_naoruzanje[2]=2;
        bot_naoruzanje[3]=1;
        bot_naoruzanje[4]=0;
        bot_naoruzanje[5]=1;
        bot_naoruzanje[6]=1;
        break;
    case 1:
        bot_naoruzanje[0]=2;
        bot_naoruzanje[1]=2;
        bot_naoruzanje[2]=2;
        bot_naoruzanje[3]=0;
        bot_naoruzanje[4]=3;
        bot[1][0]=1;
        bot[2][0]=1;
        bot[5][0]=1;
        bot[4][0]=1;
        bot[10][0]=1;
        bot[9][0]=1;
        bot_naoruzanje[5]=1;
        bot_naoruzanje[6]=1;
        break;
    case 2:
        bot_naoruzanje[0]=2;
        bot_naoruzanje[1]=0;
        bot_naoruzanje[2]=2;
        bot_naoruzanje[3]=1;
        bot_naoruzanje[4]=3;
        bot[1][0]=1;
        bot[2][0]=1;
        bot[5][0]=1;
        bot[4][0]=1;
        bot[10][0]=1;
        bot[9][0]=1;
        bot_naoruzanje[5]=1;
        bot_naoruzanje[6]=0;
        break;
    case 3:
        bot_naoruzanje[0]=2;
        bot_naoruzanje[1]=2;
        bot_naoruzanje[2]=2;
        bot_naoruzanje[3]=1;
        bot_naoruzanje[4]=1;
        bot[1][0]=1;
        bot[2][0]=1;
        bot_naoruzanje[5]=1;
        bot_naoruzanje[6]=0;
        break;
    default:
        break;
    }
    
}
pravila1()
{
    printf("Igra se na tabli 10x10.\nIma dva igraca.\nSvako ima da postavi:\n-4 broda duzine 1\n-3 broda duzine 2\n-2 broda duzine 3\n-1 brod duzine duzine 4.\nRazdaljina izmedju brodova mora biti 1.\n");
    printf("Pritisni bilo koje dugme da nastavis.\n");
}
