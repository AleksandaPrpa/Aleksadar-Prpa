                                if(prvi_igrac[red1-1][kolona2-3]==88 && prvi_igrac[red1-1][kolona2-2]==88 && prvi_igrac[red1-1][kolona2-1]==88 && prvi_igrac[red1-1][kolona2]==88 && prvi_igrac[red1-1][kolona2+1]==88 && prvi_igrac[red1][kolona2-3]==88 && prvi_igrac[red1][kolona2+1]==88 && prvi_igrac[red1+1][kolona2-3]==88 && prvi_igrac[red1+1][kolona2-2]==88 && prvi_igrac[red1+1][kolona2-1]==88 && prvi_igrac[red1+1][kolona2]==88 && prvi_igrac[red1+1][kolona2+1]==88)
