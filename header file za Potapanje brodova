            printf("%d  %c %c %c %c %c %c %c %c %c %c\n",6,prvi_igrac[6][1],prvi_igrac[6][2],prvi_igrac[6][3],prvi_igrac[6][4],prvi_igrac[6][5],prvi_igrac[6][6],prvi_igrac[6][7],prvi_igrac[6][8],prvi_igrac[6][9],prvi_igrac[6][10]);
            printf("%d  %c %c %c %c %c %c %c %c %c %c\n",7,prvi_igrac[7][1],prvi_igrac[7][2],prvi_igrac[7][3],prvi_igrac[7][4],prvi_igrac[7][5],prvi_igrac[7][6],prvi_igrac[7][7],prvi_igrac[7][8],prvi_igrac[7][9],prvi_igrac[7][10]);
            printf("%d  %c %c %c %c %c %c %c %c %c %c\n",8,prvi_igrac[8][1],prvi_igrac[8][2],prvi_igrac[8][3],prvi_igrac[8][4],prvi_igrac[8][5],prvi_igrac[8][6],prvi_igrac[8][7],prvi_igrac[8][8],prvi_igrac[8][9],prvi_igrac[8][10]);
