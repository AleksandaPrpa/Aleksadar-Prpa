void ispis_table()
{
    int tabla[11][11];
    tabla[0][1] = 1;
    tabla[0][2] = 2;
    tabla[0][3] = 3;
    tabla[0][4] = 4;
    tabla[0][5] = 5;
    tabla[0][6] = 6;
    tabla[0][7] = 7;
    tabla[0][8] = 8;
    tabla[0][9] = 9;
    tabla[0][10] = 10;

    tabla[1][0] = 1;
    tabla[2][0] = 2;
    tabla[3][0] = 3;
    tabla[4][0] = 4;
    tabla[5][0] = 5;
    tabla[6][0] = 6;
    tabla[7][0] = 7;
    tabla[8][0] = 8;
    tabla[9][0] = 9;
    tabla[10][0] = 10;
    printf("%c   %d %d %d %d %d %d %d %d %d %d\n", 0, tabla[0][1], tabla[0][2], tabla[0][3], tabla[0][4], tabla[0][5], tabla[0][6], tabla[0][7], tabla[0][8], tabla[0][9], tabla[0][10]);
    printf("%d  %c %c %c %c %c %c %c %c %c %c\n", tabla[1][0], 88, 88, 88, 88, 88, 88, 88, 88, 88, 88);
    printf("%d  %c %c %c %c %c %c %c %c %c %c\n", tabla[2][0], 88, 88, 88, 88, 88, 88, 88, 88, 88, 88);
    printf("%d  %c %c %c %c %c %c %c %c %c %c\n", tabla[3][0], 88, 88, 88, 88, 88, 88, 88, 88, 88, 88);
    printf("%d  %c %c %c %c %c %c %c %c %c %c\n", tabla[4][0], 88, 88, 88, 88, 88, 88, 88, 88, 88, 88);
    printf("%d  %c %c %c %c %c %c %c %c %c %c\n", tabla[5][0], 88, 88, 88, 88, 88, 88, 88, 88, 88, 88);
    printf("%d  %c %c %c %c %c %c %c %c %c %c\n", tabla[6][0], 88, 88, 88, 88, 88, 88, 88, 88, 88, 88);
    printf("%d  %c %c %c %c %c %c %c %c %c %c\n", tabla[7][0], 88, 88, 88, 88, 88, 88, 88, 88, 88, 88);
    printf("%d  %c %c %c %c %c %c %c %c %c %c\n", tabla[8][0], 88, 88, 88, 88, 88, 88, 88, 88, 88, 88);
