            printf("%d  %c %c %c %c %c %c %c %c %c %c\n",9,prvi_igrac[9][1],prvi_igrac[9][2],prvi_igrac[9][3],prvi_igrac[9][4],prvi_igrac[9][5],prvi_igrac[9][6],prvi_igrac[9][7],prvi_igrac[9][8],prvi_igrac[9][9],prvi_igrac[9][10]);
