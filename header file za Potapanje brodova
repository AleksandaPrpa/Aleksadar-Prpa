    printf("%d  %c %c %c %c %c %c %c %c %c %c\n",4,prvi_igrac[4][1],prvi_igrac[4][2],prvi_igrac[4][3],prvi_igrac[4][4],prvi_igrac[4][5],prvi_igrac[4][6],prvi_igrac[4][7],prvi_igrac[4][8],prvi_igrac[4][9],prvi_igrac[4][10]);
