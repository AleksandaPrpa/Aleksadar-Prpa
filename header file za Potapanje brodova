bot()
{
    pravila2();
    getch();
    system("cls");
    int i, j,y=1; // brojaci  y-brojac broda
    int n=12,m=12;
    int potez = 0, rezultat = 0,potop1=0,potop2=0;
    int red1,kolona1;
    int prvi_igrac[n][m];
    int bot[n][m];
    int prvi_igrac_sakriveno[n][m];
    int bot_sakriveno[n][m];
    system("cls");
    for(i=0;i<12;i++)
    {
        for(j=0;j<12;j++)
        {
            prvi_igrac[i][j]=88;//
            bot[i][j]=88;
            prvi_igrac_sakriveno[i][j]=88;
            bot_sakriveno[i][j]=88;
        }
    }
    printf("Igrac namesta brodove\n");
    ispis_table();
    unos_brodova(n,m,prvi_igrac);
    printf("Pritisni bilo koje dugme da nastavis.\n");
    getch();
    system("cls");
    unos_brodova_bot(n,m,bot);
	while (rezultat == 0)
    {
        if (potez % 2 == 0)
        {
            system("cls");
            printf("Tabla od Bota\n");
            isaisdas(n,m,bot_sakriveno);
            printf("Igrac igra\n");
            switch (provera(n,m,bot,bot_sakriveno))
            {
            case 0:
                printf("Promasaj\n");
                getch();
                system("cls");
                break;
            case 1:
                potez++;
                break;
            case 2:
                potez++;
                potop1++;
                break;
            default:
                break;
            }
            system("cls");
            isaisdas(n,m,bot_sakriveno);
            if(potop1==10)
            {
                rezultat++;
                system("cls");
                printf("Igrac je pobednik\nPostava brodova od igraca je\n");
                isaisdas(n,m,prvi_igrac_sakriveno);
                kraj();
            }
        }
        else
        {
            system("cls");
            printf("Tabla od Igraca\n");
            isaisdas(n,m,prvi_igrac_sakriveno);
            printf("Bot igra\n");
            getch();
            switch (provera_bot(n,m,prvi_igrac,prvi_igrac_sakriveno))
            {
            case 0:
                getch();
                break;
            case 1:
                potez++;
                getch();
                break;
            case 2:
                potez++;
                potop2++;
                getch();
                break;
            default:
                break;
            }
            system("cls");
            isaisdas(n,m,prvi_igrac_sakriveno);
            getch();
            if(potop2==10)
            {
                rezultat++;
                system("cls");
                printf("Bot je pobednik\nPostava brodova od Bota je\n");
                isaisdas(n,m,bot);
                kraj();
            }
        }
       potez++;
    }
}
provera_bot(int n,int m,int drugi_igrac[n][m],int drugi_igrac_sakriveno[n][m])
{
    int y,potop1,red1,kolona1,potez;
    p:
    srand(time(NULL));
    red1=rand()%10+1;
    kolona1=rand()%10+1;
    if(drugi_igrac[red1][kolona1]==79 || drugi_igrac[red1][kolona1]==335)
    {
        system("cls");
        goto p;
    }
    if(drugi_igrac[red1][kolona1]!=88)
    {
                potez++;
                drugi_igrac_sakriveno[red1][kolona1]=79;
                drugi_igrac[red1][kolona1]=335;
                if(drugi_igrac[red1-1][kolona1-1]!=30 && drugi_igrac[red1-1][kolona1]!=30 && drugi_igrac[red1-1][kolona1+1]!=30 && drugi_igrac[red1][kolona1-1]!=30 && drugi_igrac[red1][kolona1+1]!=30 && drugi_igrac[red1+1][kolona1-1]!=30 && drugi_igrac[red1+1][kolona1]!=30 && drugi_igrac[red1+1][kolona1+1]!=30)
                {
                    if(drugi_igrac[red1-1][kolona1-1]!=17 && drugi_igrac[red1-1][kolona1]!=17 && drugi_igrac[red1-1][kolona1+1]!=17 && drugi_igrac[red1][kolona1-1]!=17 && drugi_igrac[red1][kolona1+1]!=17 && drugi_igrac[red1+1][kolona1-1]!=17 && drugi_igrac[red1+1][kolona1]!=17 && drugi_igrac[red1+1][kolona1+1]!=17)
                    {
                        if(drugi_igrac[red1-1][kolona1-1]!=254 && drugi_igrac[red1-1][kolona1]!=254 && drugi_igrac[red1-1][kolona1+1]!=254 && drugi_igrac[red1][kolona1-1]!=254 && drugi_igrac[red1][kolona1+1]!=254 && drugi_igrac[red1+1][kolona1-1]!=254 && drugi_igrac[red1+1][kolona1]!=254 && drugi_igrac[red1+1][kolona1+1]!=254)
                        {
                            y=1;
                            potop1++;
                            if(drugi_igrac[red1][kolona1-1]==335 || drugi_igrac[red1][kolona1+1]==335)
                            {
                                if(drugi_igrac[red1][kolona1-1]==335)
                                {
                                    y++;
                                    if(drugi_igrac[red1][kolona1-2]==335)
                                    {
                                        y++;
                                        if(drugi_igrac[red1][kolona1-3]==335)
                                        {
                                            y++;
                                            drugi_igrac[red1+1][kolona1-4]=79;
                                            drugi_igrac[red1+1][kolona1-3]=79;
                                            drugi_igrac[red1+1][kolona1-2]=79;
                                            drugi_igrac[red1+1][kolona1-1]=79;
                                            drugi_igrac[red1+1][kolona1]=79;
                                            drugi_igrac[red1+1][kolona1+1]=79;

                                            drugi_igrac[red1][kolona1-4]=79;
                                            drugi_igrac[red1][kolona1+1]=79;

                                            drugi_igrac[red1-1][kolona1-4]=79;
                                            drugi_igrac[red1-1][kolona1-3]=79;
                                            drugi_igrac[red1-1][kolona1-2]=79;
                                            drugi_igrac[red1-1][kolona1-1]=79;
                                            drugi_igrac[red1-1][kolona1]=79;
                                            drugi_igrac[red1-1][kolona1+1]=79;
                                            
                                            drugi_igrac_sakriveno[red1+1][kolona1-4]=79;
                                            drugi_igrac_sakriveno[red1+1][kolona1-3]=79;
                                            drugi_igrac_sakriveno[red1+1][kolona1-2]=79;
                                            drugi_igrac_sakriveno[red1+1][kolona1-1]=79;
                                            drugi_igrac_sakriveno[red1+1][kolona1]=79;
                                            drugi_igrac_sakriveno[red1+1][kolona1+1]=79;

                                            drugi_igrac_sakriveno[red1][kolona1-4]=79;
                                            drugi_igrac_sakriveno[red1][kolona1+1]=79;

                                            drugi_igrac_sakriveno[red1-1][kolona1-4]=79;
