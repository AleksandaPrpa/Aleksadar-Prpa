    printf("%d  %c %c %c %c %c %c %c %c %c %c\n",9,prvi_igrac[9][1],prvi_igrac[9][2],prvi_igrac[9][3],prvi_igrac[9][4],prvi_igrac[9][5],prvi_igrac[9][6],prvi_igrac[9][7],prvi_igrac[9][8],prvi_igrac[9][9],prvi_igrac[9][10]);
    printf("%d %c %c %c %c %c %c %c %c %c %c\n",10,prvi_igrac[10][1],prvi_igrac[10][2],prvi_igrac[10][3],prvi_igrac[10][4],prvi_igrac[10][5],prvi_igrac[10][6],prvi_igrac[10][7],prvi_igrac[10][8],prvi_igrac[10][9],prvi_igrac[10][10]);
    }
    //brod duzine 3
    {
        for(br3=0;br3<2;br3++)
        {
            printf("Brod duzine 3\nUnesi od kog polja do kog polja ce da bude\nPrvo unosis red %c pa kolonu %c\n",281,282);
            c:
            printf("Od\n");
            red1= unos_polja();
            kolona1=unos_polja();
            printf("Do\n");
            red2= unos_polja();
            kolona2=unos_polja();
            if(red1==red2 && kolona1==kolona2)
            {
                printf("To je brod duzine 1.\nUnesi ponovo\n");
                goto c;
            }
            else
            {
                if(red1==red2 || kolona1==kolona2)
                {
                    if(abs(red1-red2)==2 || abs(kolona1-kolona2)==2)
                    {
                        if(red1==red2)
                        {
                            if(kolona1>kolona2)
                            {
                                if(prvi_igrac[red1-1][kolona1-3]==88 && prvi_igrac[red1-1][kolona1-2]==88 && prvi_igrac[red1-1][kolona1-1]==88 && prvi_igrac[red1-1][kolona1]==88 && prvi_igrac[red1-1][kolona1+1]==88 && prvi_igrac[red1][kolona1-3]==88 && prvi_igrac[red1][kolona1+1]==88 && prvi_igrac[red1+1][kolona1-3]==88 && prvi_igrac[red1+1][kolona1-2]==88 && prvi_igrac[red1+1][kolona1-1]==88 && prvi_igrac[red1+1][kolona1]==88 && prvi_igrac[red1+1][kolona1+1]==88)
                                {
                                    if(prvi_igrac[red1][kolona1-2]!=17 && prvi_igrac[red1][kolona1-1]!=17 && prvi_igrac[red1][kolona1]!=17 && prvi_igrac[red1][kolona1-2]!=30 && prvi_igrac[red1][kolona1-1]!=30 && prvi_igrac[red1][kolona1]!=30)
                                    {
                                        if(prvi_igrac[red1][kolona1-2]!=254 && prvi_igrac[red1][kolona1-1]!=254 && prvi_igrac[red1][kolona1]!=254)
                                        {
                                            prvi_igrac[red1][kolona1]=254;
                                            prvi_igrac[red1][kolona1-1]=254;
                                            prvi_igrac[red1][kolona1-2]=17;
                                        }
                                        else
                                        {
                                            printf("Ne mogu dva broda da budu na istom polju\n");
                                            goto c;   
                                        }
                                    }
                                    else
