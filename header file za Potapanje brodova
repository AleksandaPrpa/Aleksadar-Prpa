            printf("%d %c %c %c %c %c %c %c %c %c %c\n",10,prvi_igrac[10][1],prvi_igrac[10][2],prvi_igrac[10][3],prvi_igrac[10][4],prvi_igrac[10][5],prvi_igrac[10][6],prvi_igrac[10][7],prvi_igrac[10][8],prvi_igrac[10][9],prvi_igrac[10][10]);
		}
	}
    //brod duzine 1
    {
        for(br1=0;br1<4;br1++)
        {
            printf("Brod duzine 1\nUnesi polje\n");
            e:
            red1= unos_polja();
            kolona1=unos_polja();
            if(prvi_igrac[red1-1][kolona1-1]==88 && prvi_igrac[red1-1][kolona1]==88 && prvi_igrac[red1-1][kolona1+1]==88 && prvi_igrac[red1][kolona1-1]==88 && prvi_igrac[red1][kolona1+1]==88 && prvi_igrac[red1+1][kolona1-1]==88 && prvi_igrac[red1+1][kolona1]==88 && prvi_igrac[red1+1][kolona1+1]==88)
            {
                if(prvi_igrac[red1][kolona1]!=17 && prvi_igrac[red1][kolona1]!=30)
                {
                    if(prvi_igrac[red1][kolona1]!=254)
                    {
                        prvi_igrac[red1][kolona1]=17;
                    }
                    else
                    {
                        printf("Ne mogu dva broda da budu na istom polju\n");
                        goto e;   
                    }
                }
